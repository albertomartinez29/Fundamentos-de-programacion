//* 2. mostrar los 5 primeros numeros multiplos de 5 

Algoritmo numerosMultiplos5
    Escribir "Estos son los 5 primemors numeros  multiplos de 5:" 
    con <- 5
    Mientras con <= 25 Hacer
        Escribir con,"  " Sin Saltar
        con <- con + 5
    FinMientras
    Escribir " "         //* salto de linea
FinAlgoritmo